<%- include("partials/header") -%>

<div class="loginform">
    <h1>login</h1>
    <form action="/login" method="post">
        <label for="loginusr">unique username</label>
        <input type="text" id="loginusr" name="username" autocomplete="off" required autofocus>
        <label for="loginpass">password</label>
        <input type="password" id="loginpass" name="password" autocomplete="off" required>
        <button type="submit" name="sub">submit</button>
    </form>
</div>


<%- include("partials/footer") -%>